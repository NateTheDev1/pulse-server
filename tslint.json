{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-string-literal": false,
        "no-console": false,
        "forin": false,
        "no-shadowed-variable": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules",
            "example",
            "./src/__tests__/**/*"
        ]
    }
}